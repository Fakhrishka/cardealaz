<table>
  <thead>
    <tr>
      <th>Brand</th>
      <th>Model</th>
      <th>Test drive</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @allcars.each do | brand, cars| %>
        <% cars.each do |car|%>
		<tr>
       		<td><%= brand %></td>
			<td><%=car['model']%></td>
			<td><%= 'Available' if car['test_drive']%></td>
		</tr>
		<%end%>
    <% end %>
  </tbody>
</table>