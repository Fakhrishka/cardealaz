    <h1>Users</h1>

    <table>
      <thead>
        <tr>
          <th>Login</th>
          <th>password</th>
          <th colspan="2"></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= user.login %></td>
            <td><%= user.password %></td>
          </tr>
        <% end %>
      </tbody>
    </table>